{
  "exclude": [
    "node_modules",
    "templates/*"
  ],
  // "include": ["./**/*"],
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "jsx": "react",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "preserveWatchOutput": true
  }
}