{
  "name": "daren-sanity-studio",
  "author": "Daren Malfait <me@daren.be> (https://daren.be)",
  "scripts": {
    "build": "cross-env SANITY_ACTIVE_ENV=production npm run tailwind:build && sanity build",
    "deploy": "cross-env SANITY_ACTIVE_ENV=production npm run tailwind:build && sanity deploy",
    "dev": "cp ../.env .env.development && cross-env SANITY_ACTIVE_ENV=development npm run tailwind:build && sanity start",
    "export": "npx sanity dataset export production --no-drafts ../data/production.tar.gz",
    "import": "npx sanity dataset import ../data/production.tar.gz --missing",
    "lint:types": "tsc --noEmit",
    "tailwind:build": "npx tailwindcss build ./styles/tailwind.css -o ./styles/tailwind.output.css",
    "test": "sanity check",
    "validate": "concurrently --kill-others-on-fail --prefix 'lint:studio' npm:lint:*"
  },
  "dependencies": {
