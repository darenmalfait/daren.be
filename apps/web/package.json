{
  "name": "nerdfish.be",
  "private": true,
  "description": "full stack web developer",
  "author": "Daren Malfait <daren@nerdfish.be> (https://nerdfish.be)",
  "type": "module",
  "scripts": {
    "build": "dotenv -e ../../.env tinacms build -- -c \"dotenv -e ../../.env next build\"",
    "clean": "rm -rf .next dist node_modules .turbo",
    "dev": "tinacms dev --verbose -c \"dotenv -e ../../.env next dev\"",
    "format": "prettier . --check --cache --cache-location=\"node_modules/.cache/prettiercache\"",
    "format:fix": "prettier . --write --cache --cache-location=\"node_modules/.cache/prettiercache\" --log-level=warn",
    "lint:fix": "eslint . --fix --cache --cache-location \"node_modules/.cache/.eslintcache\" --max-warnings 0",
    "lint": "eslint . --max-warnings 0",
    "preinstall": "npx only-allow pnpm",
    "start": "tinacms dev -c \"dotenv -e ../../.env next start\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.66",
    "@calcom/embed-react": "^1.5.1",
    "@formatjs/intl-localematcher": "^0.5.5",
    "@hookform/resolvers": "^3.9.0",
    "@nerdfish-website/emails": "workspace:*",
    "@nerdfish-website/lib": "workspace:*",
    "@nerdfish-website/ui": "workspace:*",
    "@nerdfish/tailwind-config": "^3.4.0",
    "@nerdfish/theme": "^2.3.0",
    "@nerdfish/ui": "^7.3.5",
    "@nerdfish/utils": "^2.0.0",
    "@next/env": "^14.2.13",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.15",
    "@tinacms/auth": "^1.0.8",
    "@vercel/og": "^0.6.3",
    "ai": "^3.4.10",
    "date-fns": "^3.6.0",
    "feed": "^4.2.2",
    "framer-motion": "^11.11.8",
    "geist": "^1.3.1",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.4",
    "negotiator": "^1.0.0",
    "next": "^14.2.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-obfuscate": "^3.7.0",
    "resend": "^3.5.0",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "tailwindcss-animate": "^1.0.7",
    "tinacms": "^2.2.9",
    "tiny-invariant": "^1.3.3",
    "typescript": "^5.6.3",
    "use-scramble": "^2.2.15",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tinacms/cli": "^1.6.9",
    "@types/lodash": "^4.17.10",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20.16.11",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/styled-components": "^5.1.34",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.11.1",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.6",
    "npm-run-all2": "^6.2.3",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.0",
    "prettier": "^3.3.3",
    "shelljs": "^0.8.5",
    "tailwindcss": "^3.4.13"
  },
  "packageManager": "pnpm@9.12.1"
}
