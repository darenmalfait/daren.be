{
  "name": "nerdfish.be",
  "private": true,
  "description": "full stack web developer",
  "author": "Daren Malfait <daren@nerdfish.be> (https://nerdfish.be)",
  "type": "module",
  "scripts": {
    "build": "dotenv -e ../../.env tinacms build -- -c \"dotenv -e ../../.env next build\"",
    "clean": "rm -rf .next dist node_modules .turbo",
    "dev": "tinacms dev --verbose -c \"dotenv -e ../../.env next dev\"",
    "format": "prettier . --check --cache --cache-location=\"node_modules/.cache/prettiercache\"",
    "format:fix": "prettier . --write --cache --cache-location=\"node_modules/.cache/prettiercache\" --log-level=warn",
    "lint:fix": "eslint . --fix --cache --cache-location \"node_modules/.cache/.eslintcache\" --max-warnings 0",
    "lint": "eslint . --max-warnings 0",
    "preinstall": "npx only-allow pnpm",
    "start": "tinacms dev -c \"dotenv -e ../../.env next start\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@nerdfish-website/emails": "workspace:*",
    "@nerdfish-website/lib": "workspace:*",
    "@nerdfish-website/ui": "workspace:*",
    "@nerdfish/tailwind-config": "^2.2.4",
    "@nerdfish/theme": "^2.1.8",
    "@nerdfish/ui": "^5.0.2",
    "@nerdfish/utils": "^1.6.6",
    "@next/env": "^14.2.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.13",
    "@tinacms/auth": "^1.0.6",
    "@vercel/og": "^0.6.2",
    "date-fns": "^3.6.0",
    "feed": "^4.2.2",
    "framer-motion": "^11.3.12",
    "geist": "^1.3.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.414.0",
    "match-sorter": "^6.3.4",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-obfuscate": "^3.6.9",
    "react-syntax-highlighter": "^15.5.0",
    "resend": "^3.5.0",
    "sharp": "^0.33.4",
    "slugify": "^1.6.6",
    "tailwindcss-animate": "^1.0.7",
    "tinacms": "^1.6.7",
    "tiny-invariant": "^1.3.3",
    "typescript": "^5.5.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tinacms/cli": "^1.5.52",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/styled-components": "^5.1.34",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.6.0",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.1",
    "npm-run-all2": "^6.2.2",
    "postcss": "^8.4.39",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^12.1.5",
    "prettier": "^3.3.2",
    "shelljs": "^0.8.5",
    "tailwindcss": "^3.4.4"
  },
  "packageManager": "pnpm@9.6.0"
}
