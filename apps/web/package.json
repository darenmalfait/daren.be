{
  "name": "nerdfish.be",
  "private": true,
  "description": "full stack web developer",
  "author": "Daren Malfait <daren@nerdfish.be> (https://nerdfish.be)",
  "scripts": {
    "build": "dotenv -e ../../.env tinacms build -- -c \"dotenv -e ../../.env next build\"",
    "clean": "rm -rf .next dist node_modules .turbo",
    "dev": "tinacms dev --verbose -c \"dotenv -e ../../.env next dev\"",
    "format": "prettier . --check --cache --cache-location=\"node_modules/.cache/prettiercache\"",
    "format:fix": "prettier . --write --cache --cache-location=\"node_modules/.cache/prettiercache\" --log-level=warn",
    "lint:fix": "eslint . --fix --cache --cache-location \"node_modules/.cache/.eslintcache\" --max-warnings 0",
    "lint": "eslint . --max-warnings 0",
    "preinstall": "npx only-allow pnpm",
    "start": "tinacms dev -c \"dotenv -e ../../.env next start\"",
    "typecheck": "tsc --noEmit"
  },
  "resolutions": {
    "@headlessui/react": "^1.7.18"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@hookform/resolvers": "^3.3.4",
    "@nerdfish-website/emails": "workspace:*",
    "@nerdfish-website/lib": "workspace:*",
    "@nerdfish-website/lint": "workspace:*",
    "@nerdfish-website/tsconfig": "workspace:*",
    "@nerdfish-website/ui": "workspace:*",
    "@nerdfish/tailwind-config": "^2.2.1",
    "@nerdfish/theme": "^2.1.5",
    "@nerdfish/ui": "^4.6.2",
    "@nerdfish/utils": "^1.6.4",
    "@next/env": "^14.1.4",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.12",
    "@tinacms/auth": "^1.0.4",
    "@vercel/og": "^0.6.2",
    "date-fns": "^3.6.0",
    "feed": "^4.2.2",
    "framer-motion": "^11.0.25",
    "geist": "^1.3.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.365.0",
    "match-sorter": "^6.3.4",
    "next": "^14.1.4",
    "next-tinacms-cloudinary": "^5.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.24",
    "react-hook-form": "^7.51.2",
    "react-obfuscate": "^3.6.9",
    "react-syntax-highlighter": "^15.5.0",
    "resend": "^3.2.0",
    "sharp": "^0.33.3",
    "slugify": "^1.6.6",
    "tinacms": "^1.6.1",
    "tiny-invariant": "^1.3.3",
    "typescript": "^5.4.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tinacms/cli": "^1.5.43",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.5",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
