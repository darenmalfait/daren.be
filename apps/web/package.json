{
  "name": "nerdfish.be",
  "private": true,
  "description": "full stack web developer",
  "author": "Daren Malfait <daren@nerdfish.be> (https://nerdfish.be)",
  "type": "module",
  "scripts": {
    "build": "dotenv -e ../../.env tinacms build -- -c \"dotenv -e ../../.env next build\"",
    "clean": "rm -rf .next dist node_modules .turbo",
    "dev": "tinacms dev --verbose -c \"dotenv -e ../../.env next dev\"",
    "format": "prettier . --check --cache --cache-location=\"node_modules/.cache/prettiercache\"",
    "format:fix": "prettier . --write --cache --cache-location=\"node_modules/.cache/prettiercache\" --log-level=warn",
    "lint:fix": "eslint . --fix --cache --cache-location \"node_modules/.cache/.eslintcache\" --max-warnings 0",
    "lint": "eslint . --max-warnings 0",
    "preinstall": "npx only-allow pnpm",
    "start": "tinacms dev -c \"dotenv -e ../../.env next start\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.72",
    "@calcom/embed-react": "^1.5.1",
    "@formatjs/intl-localematcher": "^0.5.8",
    "@hookform/resolvers": "^3.9.1",
    "@nerdfish-website/email": "workspace:*",
    "@nerdfish-website/env": "workspace:*",
    "@nerdfish-website/lib": "workspace:*",
    "@nerdfish-website/observability": "workspace:*",
    "@nerdfish-website/seo": "workspace:*",
    "@nerdfish-website/ui": "workspace:*",
    "@nerdfish/tailwind-config": "^3.7.2",
    "@nerdfish/theme": "^2.4.1",
    "@nerdfish/ui": "^7.6.3",
    "@nerdfish/utils": "^2.0.0",
    "@next/env": "^14.2.18",
    "@react-aria/i18n": "^3.12.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.15",
    "@tinacms/auth": "^1.0.8",
    "@vercel/og": "^0.6.4",
    "ai": "^3.4.33",
    "date-fns": "^4.1.0",
    "feed": "^4.2.2",
    "geist": "^1.3.1",
    "lodash": "^4.17.21",
    "match-sorter": "^8.0.0",
    "motion": "^11.11.17",
    "negotiator": "^1.0.0",
    "next": "^14.2.18",
    "next-transition-router": "^0.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-obfuscate": "^3.7.0",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "tailwindcss-motion": "^0.4.3-beta",
    "tinacms": "^2.4.0",
    "tiny-invariant": "^1.3.3",
    "tween-functions": "^1.2.0",
    "typescript": "^5.6.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tinacms/cli": "^1.6.11",
    "@types/lodash": "^4.17.13",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/tween-functions": "^1.2.2",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.3",
    "eslint": "^9.15.0",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.7",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.3.3",
    "shelljs": "^0.8.5",
    "tailwindcss": "^3.4.15"
  },
  "packageManager": "pnpm@9.13.2"
}
