{
  "name": "@nerdfish-website/ui",
  "version": "0.1.0",
  "private": true,
  "exports": {
    "./types": "./src/types.ts",
    "./icons": "./src/icons.tsx",
    "./components/*": "./src/components/*"
  },
  "typesVersions": {
    "*": {
      "types": [
        "./src/types.ts"
      ],
      "icons": [
        "./src/icons.tsx"
      ],
      "components/*": [
        "./src/components/*"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf node_modules .turbo",
    "format": "prettier . --check --cache --cache-location=\"node_modules/.cache/prettiercache\"",
    "format:fix": "prettier . --write --cache --cache-location=\"node_modules/.cache/prettiercache\" --log-level=warn",
    "lint": "eslint . --cache --cache-location \"node_modules/.cache/.eslintcache\" --max-warnings 0",
    "lint:fix": "eslint . --fix --cache --cache-location \"node_modules/.cache/.eslintcache\" --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "^2.0.4",
    "@mdxeditor/editor": "^3.0.8",
    "@nerdfish-website/lib": "workspace:*",
    "@nerdfish/tailwind-config": "^2.2.0",
    "@nerdfish/ui": "^4.6.0",
    "@nerdfish/utils": "^1.6.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@reach/checkbox": "^0.18.0",
    "@tailwindcss/typography": "^0.5.12",
    "chart.js": "^4.4.2",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.365.0",
    "mapbox-gl": "^3.2.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-easy-crop": "^5.0.6",
    "react-map-gl": "^7.1.7",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "slugify": "^1.6.6",
    "tailwindcss": "3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@nerdfish-website/lint": "workspace:*",
    "@nerdfish-website/tsconfig": "workspace:*",
    "@types/react": "^18.2.74",
    "@types/react-syntax-highlighter": "^15.5.11",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.4"
  }
}
